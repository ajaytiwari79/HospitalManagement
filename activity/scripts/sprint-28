// To update time from Integer to LocalDate
db.activities.updateMany({},{$set:{"rulesActivityTab.earliestStartTime":"00:00:00","rulesActivityTab.latestStartTime":"00:00:00"}})

// To update time from Integer to LocalDate in StaffActivitySetting
db.staffActivitySetting.updateMany({},{$set:{"earliestStartTime":"00:00:00","latestStartTime":"00:00:00"}})