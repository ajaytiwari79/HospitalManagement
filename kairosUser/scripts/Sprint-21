1. // TO set unit permission of Admin
Match (e:Employment) WHERE e.name="working as country admin"   WITH e
MATCH (o:Organization) WHERE o.organizationLevel="COUNTRY" WITH  o,e
CREATE (u:UnitPermission{startDate:timestamp()}) WITH o,e,u
CREATE UNIQUE (e)-[:HAS_UNIT_PERMISSIONS]->(u)-[:APPLICABLE_IN_UNIT]->(o)return e,o;

2. // TO set permission to Access Group AG_COUNTRY_ADMIN
Match (ag:AccessGroup) WHERE ag.name='AG_COUNTRY_ADMIN' WITH ag
MATCH (ap:AccessPage) WITH ap,ag
CREATE UNIQUE (ag)-[r:HAS_ACCESS_OF_TABS{isEnabled:true, read:true, write:true}]->(ap) return r,ag,ap;
