Match(ap:AccessPage) where ap.moduleId="tab_427" merge (ap)-[:SUB_PAGE]-(accessPage:AccessPage{name:"Shift Planning",isModule:false,moduleId:"tab_428",active:true});

Match (ag:AccessGroup) WHERE ag.name="SUPER_ADMIN"
Match (ap:AccessPage)
CREATE UNIQUE (ag)-[r:HAS_ACCESS_OF_TABS{isEnabled:true,read:true,write:true}]-(ap);