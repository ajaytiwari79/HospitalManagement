//Add Filter Group At groups
Match (ap:AccessPage)-[:APPLICABLE_FOR]->(fg:FilterGroup) where ap.name="Groups"
SET fg.filterTypes=["EMPLOYMENT_TYPE", "GENDER", "TAGS", "EXPERTISE", "TEAM", "SKILLS", "FUNCTIONS", "REAL_TIME_STATUS", "TIME_SLOT", "MAIN_TEAM", "ACCESS_GROUPS",
 "SKILL_LEVEL", "AGE", "ORGANIZATION_EXPERIENCE", "BIRTHDAY", "SENIORITY", "PAY_GRADE_LEVEL", "FIBONACCI", "ABSENCE_ACTIVITY",
 "TIME_BANK_BALANCE", "EMPLOYED_SINCE"]
return fg;

//Add Filter Group At SELF ROSTERING
Match (ap:AccessPage)-[:APPLICABLE_FOR]->(fg:FilterGroup) where ap.name="Self Rostering"
SET fg.filterTypes=["PLANNED_TIME_TYPE", "EMPLOYMENT_TYPE", "GENDER", "STAFF_STATUS", "EXPERTISE", "TIME_TYPE", "TAGS", "GROUPS", "NIGHT_WORKERS", "ESCALATION_CAUSED_BY", "ACTIVITY_TIMECALCULATION_TYPE", "ACTIVITY_STATUS", "REAL_TIME_STATUS", "TIME_SLOT", "TEAM", "ABSENCE_ACTIVITY", "VALIDATED_BY", "SKILLS", "FUNCTIONS"]
return fg;

//Add Filter Group At STAFF
Match (ap:AccessPage)-[:APPLICABLE_FOR]->(fg:FilterGroup) where ap.name="Staff"
SET fg.filterTypes=["EMPLOYMENT_TYPE", "GENDER", "STAFF_STATUS", "EMPLOYMENT", "EXPERTISE", "SKILLS", "TEAM", "TAGS", "GROUPS", "NIGHT_WORKERS"]
return fg;

