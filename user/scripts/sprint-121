//Add Filter Group At SELF ROSTERING
Match (ap:AccessPage)-[:APPLICABLE_FOR]->(fg:FilterGroup) where ap.name="Self Rostering"
SET fg.filterTypes=["PLANNED_TIME_TYPE", "EMPLOYMENT_TYPE", "GENDER", "STAFF_STATUS", "EXPERTISE", "TIME_TYPE", "TAGS", "GROUPS", "NIGHT_WORKERS", "ESCALATION_CAUSED_BY", "ACTIVITY_TIMECALCULATION_TYPE", "ACTIVITY_STATUS", "REAL_TIME_STATUS", "TIME_SLOT", "TEAM", "ABSENCE_ACTIVITY", "VALIDATED_BY", "STOPBRICK_DELETED_BY", "SKILLS", "FUNCTIONS","CTA_ACCOUNT_TYPE"]
return fg;